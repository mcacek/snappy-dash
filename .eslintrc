{
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
    "eol-last": 2,
    "no-delete-var": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "quotes": [2, "single", "avoid-escape"],
    "semi": 2
  }
}
